{
   "name": "keypoint"
  ,"description": "Key Point"
  ,"version": "0.0.1"
  ,"private": true
  ,"main": "server.js"
  ,"dependencies": {
    "async":     "0.1.18",
    "express":   "3.0.0",
    "jade": ">= 0.0.1",
    "less": "1.3.0",
    "underscore": "1.3.3",
    "grunt": "0.3.x",
    "grunt-coffee": "0.0.x",
    "grunt-contrib-less": "0.3.x",
    "grunt-reload": "0.2.x",
    "grunt-jslint": "0.2.x",
    "grunt-shell":"*",
    "grunt-exec":"*",
    "mongojs": "0.4.x",
    "supervisor":"*",
    "everyauth":"*",
    "node-inspector":"git://github.com/dannycoates/node-inspector.git#master"    
